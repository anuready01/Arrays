class Main {
    public static void main(String[] args) {
        int[] a={2,9,7,5,1,6};
        System.out.println(search(a,7));
    }
    public static boolean search(int[] a,int key){
        for(int i=0;i<a.length;i++){
            if(a[i]==key){
                return true;
            }
            
        }
        return false;
    }
}
